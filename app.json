{
  "expo": {
    "name": "WhatsUp",
    "slug": "whatsup_expo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/icon-512.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#1a1a2e"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.whatsup.expo",
      "infoPlist": {
        "NSCameraUsageDescription": "This app needs access to your camera for video calls",
        "NSMicrophoneUsageDescription": "This app needs access to your microphone for video calls"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#1a1a2e"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.whatsup.expo",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS",
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS"
      ]
    },
    "web": {
      "favicon": "./assets/icons/favicon.png",
      "bundler": "metro",
      "name": "WhatsUp",
      "shortName": "WhatsUp",
      "description": "Video calling app for connecting with friends and family",
      "themeColor": "#0066FF",
      "backgroundColor": "#1a1a2e"
    },
    "plugins": [
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "24b746b5-3c8b-4346-91a0-6098053d2b98"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/24b746b5-3c8b-4346-91a0-6098053d2b98"
    }
  }
}
